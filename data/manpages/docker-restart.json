{
  "command_name": {
    "main_command": "docker",
    "subcommand": "restart"
  },
  "description": "Restart one or more containers",
  "flags": [
    {
      "aliases": [
        "-t",
        "--time"
      ],
      "description": "  -t, --time int   Seconds to wait for stop before killing the container (default 10)",
      "takes_argument": true
    }
  ],
  "positional_arguments": [
    {
      "name": "CONTAINER",
      "repeated": false
    },
    {
      "name": "[CONTAINER...]",
      "repeated": true
    }
  ]
}