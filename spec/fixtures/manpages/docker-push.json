{
  "description": "Push an image or a repository to a registry",
  "flags": [
    {
      "aliases": [
        "-a",
        "--all-tags"
      ],
      "description": "  -a, --all-tags                Push all tagged images in the repository",
      "takes_argument": false
    },
    {
      "aliases": [
        "--disable-content-trust"
      ],
      "description": "      --disable-content-trust   Skip image signing (default true)",
      "takes_argument": false
    },
    {
      "aliases": [
        "-q",
        "--quiet"
      ],
      "description": "  -q, --quiet                   Suppress verbose output",
      "takes_argument": false
    }
  ],
  "positional_arguments": [
    {
      "name": "[OPTIONS]",
      "repeated": false
    },
    {
      "name": "NAME[:TAG]",
      "repeated": false
    }
  ]
}