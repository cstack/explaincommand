{
  "command_name": {
    "main_command": "docker",
    "subcommand": "kill"
  },
  "description": "Kill one or more running containers",
  "flags": [
    {
      "aliases": [
        "-s",
        "--signal"
      ],
      "description": "  -s, --signal string   Signal to send to the container (default \"KILL\")",
      "takes_argument": true
    }
  ],
  "positional_arguments": [
    {
      "name": "CONTAINER",
      "repeated": false
    },
    {
      "name": "[CONTAINER...]",
      "repeated": true
    }
  ]
}